#JAVA_HOME=$HOME/java_1.4
# Grep
export GREP_OPTIONS='--color=auto' 
export GREP_COLOR='6;35'
export IRB_COLOR=true
# *NIX Editors
export EDITOR="vim"

# Evented Mongrel
export EVENT=1

# Bash History Control
export HISTCONTROL=erasedups
export HISTSIZE=5000

export LC_CTYPE=en_US.UTF-8
export HOSTFILE='/etc/hosts'
export RC_ARCHS=x86_64
export PYTHONPATH=$HOME/lib/python2.7/site-packages
export INFOPATH=.:$HOME/info:/usr/local/share/info
export CLOJURESCRIPT_HOME=$HOME/src/clojurescript
export NXJ_HOME=/usr/local/leJOS
